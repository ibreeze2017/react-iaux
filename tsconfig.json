{
  "compilerOptions": {
    "sourceMap": false,
    "skipLibCheck":true,
    "noImplicitAny": true,
    "noUnusedParameters": false,
    "jsx": "react",
    "module": "commonjs",
    "target": "es6",
    "baseUrl": ".",
    "lib": [
      "dom",
      "es5",
      "es2017",
      "es7"],
    "paths": {
      "react-iaux":["es/*"],
      "@":["docs/pages/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
